.stop-over ._em {
	border: 1rpx solid #fff;
	height: 44rpx;
	line-height: 44rpx;
	border-radius: 44rpx;
	width: 140rpx;
	position: relative;
}
.stop-over ._em::after {
	content: '';
	position: absolute;
	top: 20rpx;
	right: -30rpx;
	width: 30rpx;
	height: 2rpx;
	background: #fff;
}
.stop-over ._em::before {
	content: '';
	position: absolute;
	top: 20rpx;
	left: -30rpx;
	width: 30rpx;
	height: 2rpx;
	background: #fff;
}
